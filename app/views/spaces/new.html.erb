<%= form_for @space do | f | %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :price %>
<%= f.number_field :price %>
<%= f.label :location %>
<%= f.text_field :location %>
<%= f.label :size %>
<%= f.number_field :size %>
<%= f.label :property_type %>
<%= f.text_field :property_type %>
<%= f.submit %>
<% end %>